[
  {
    "page": "Dashboard Page",
    "elements": [
      {
        "name": "startNewJobForm",
        "type": "click",
        "locator": "//button[normalize-space()='Start']"
      },
      {
        "name": "createNewJob",
        "type": "click",
        "locator": ".x-nav-item.x-create-job-nav a"
      },
      {
        "name": "jobsNavBar",
        "type": "click",
        "locator": "//ul[@aria-label='X0PA Recruiter']//b[normalize-space(text())='Jobs']"
      },
      {
        "name": "navBarOptions",
        "type": "arrayContains",
        "locator": ".bx--header__menu-bar b"
      }
    ]
  },
  {
    "page": "Job Creation Page",
    "elements": [
      {
        "name": "jobCreatedSuccessfullyMessage",
        "type": "text",
        "locator": ".bx--tile .bx--type-alpha"
      },
      {
        "name": "submitBtn",
        "type": "click",
        "locator": "button:has-text('Submit')"
      },
      {
        "name": "additionalAttachments",
        "type": "upload",
        "locator": "div[class='bx--file__drop-container'] input"
      },
      {
        "name": "personaBenchmarkEmployee",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Employee')) input[type='text']",
        "defaultData": "",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "jobRequirement",
        "type": "editor",
        "locator": "//div[@data-placeholder='Enter requirements']"
      },
      {
        "name": "jobDescription",
        "type": "editor",
        "locator": "//div[@data-placeholder='Enter description']"
      },
      {
        "name": "skills",
        "type": "multiSelectCreate",
        "locator": "div.bx--row:has(span:has-text('Skills')) input[type='text']",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "keepJobOpenForever",
        "type": "checkbox",
        "locator": "div.bx--row:has(span:has-text('Close Date')) span:has-text('Keep job open forever until manually closed')"
      },
      {
        "name": "closeDate",
        "type": "date",
        "comment": "default data is dynamic",
        "locator": "div.bx--row:has(span:has-text('Close Date')) input[type='text']"
      },
      {
        "name": "noOfVacancies",
        "type": "input",
        "locator": "div.bx--row:has(span:has-text('No of vacancies')) input[type='number']",
        "defaultData": "5"
      },
      {
        "name": "employmentType",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Employment Type')) input[type='text']",
        "defaultData": "Full-Time",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "jobType",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Job Type')) input[type='text']",
        "defaultData": "Trainees Full Time",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "salaryRangeType",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Salary Range Type')) input[type='text']",
        "defaultData": "Core",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "maxSalary",
        "type": "input",
        "locator": "#maxSalary",
        "defaultData": "900000"
      },
      {
        "name": "minSalary",
        "type": "input",
        "locator": "#minSalary",
        "defaultData": "500000"
      },
      {
        "name": "currency",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Currency')) input[type='text']",
        "defaultData": "Singapore Dollar",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "maxExperience",
        "type": "input",
        "locator": "div.bx--row:has(span:has-text('Experience Level')) input[type='number'] >> nth=1",
        "defaultData": "6"
      },
      {
        "name": "minExperience",
        "type": "input",
        "locator": "div.bx--row:has(span:has-text('Experience Level')) input[type='number'] >> nth=0",
        "defaultData": "2"
      },
      {
        "name": "minQualification",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Min. Qualification')) input[type='text']",
        "defaultData": "Bachelor Degree",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "city",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('City')) input[type='text']",
        "defaultData": "",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "country",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Country')) input[type='text']",
        "defaultData": "Singapore",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "reportingOfficer",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Reporting Officer')) input[type='text']",
        "defaultData": "",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "hiringManager",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Hiring Manager')) input[type='text']",
        "defaultData": "",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "requireJobApprovalsToggle",
        "type": "toggleState",
        "locator": "div.bx--row:has(span:has-text('Does this job require approvals?')) label.bx--toggle-input__label",
        "onLocator": "div.bx--row:has(span:has-text('Does this job require approvals?')) .bx--toggle__text--on",
        "offLocator": "div.bx--row:has(span:has-text('Does this job require approvals?')) .bx--toggle__text--off",
        "defaultData": "No"
      },
      {
        "name": "jobStatus",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Job Status')) input[type='text']",
        "defaultData": "Internal & External",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "jobConfidentialToggle",
        "type": "toggleState",
        "locator": "div.bx--row:has(span:has-text('Is Confidential Job?')) label.bx--toggle-input__label",
        "onLocator": "div.bx--row:has(span:has-text('Is Confidential Job?')) .bx--toggle__text--on",
        "offLocator": "div.bx--row:has(span:has-text('Is Confidential Job?')) .bx--toggle__text--off",
        "defaultData": "No"
      },
      {
        "name": "acceptApplicantsOtherCountriesToggle",
        "type": "toggleState",
        "locator": "div.bx--row:has(span:has-text('Accept Applicants From Other Countries')) label.bx--toggle-input__label",
        "onLocator": "div.bx--row:has(span:has-text('Accept Applicants From Other Countries')) .bx--toggle__text--on",
        "offLocator": "div.bx--row:has(span:has-text('Accept Applicants From Other Countries')) .bx--toggle__text--off",
        "defaultData": "Yes"
      },
      {
        "name": "ownerNameVisibilityToggle",
        "type": "toggleState",
        "locator": "div.bx--row:has(span:has-text('Display job owner name in job posting?')) label.bx--toggle-input__label",
        "onLocator": "div.bx--row:has(span:has-text('Display job owner name in job posting?')) .bx--toggle__text--on",
        "offLocator": "div.bx--row:has(span:has-text('Display job owner name in job posting?')) .bx--toggle__text--off",
        "defaultData": "Yes"
      },
      {
        "name": "jobVisibilityToggle",
        "type": "toggleState",
        "locator": "div.bx--row:has(span:has-text('Job Visibility')) label.bx--toggle-input__label",
        "onLocator": "div.bx--row:has(span:has-text('Job Visibility')) .bx--toggle__text--on",
        "offLocator": "div.bx--row:has(span:has-text('Job Visibility')) .bx--toggle__text--off",
        "defaultData": "Private"
      },
      {
        "name": "jobTitle",
        "type": "input",
        "locator": "div.bx--row:has(span:has-text('Job Title')) input[type='text']",
        "defaultData": ""
      },
      {
        "name": "linkedInCompanyPage",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Post this job to the LinkedIn Company Page of')) input[type='text']",
        "defaultData": "X0PA",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "companyName",
        "type": "autocomplete",
        "locator": "div.bx--row:has(span:has-text('Company Name')) input[type='text']",
        "defaultData": "X0PA",
        "dropdownLocator": "[id^='react-select-'][id*='-option-']"
      },
      {
        "name": "jobDescriptionPDF",
        "type": "upload",
        "locator": ".bx--file__drop-container.xpa-file-cont-h input[type='file']"
      },
      {
        "name": "formHeading",
        "type": "text",
        "locator": "//h5[@class='text-uppercase mt-2']"
      }
    ]
  }
]